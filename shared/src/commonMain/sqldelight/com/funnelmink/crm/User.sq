CREATE TABLE SQLUser (
    id TEXT PRIMARY KEY,
    email TEXT UNIQUE NOT NULL,
    username TEXT NOT NULL,
    isDevAccount INTEGER NOT NULL
);

insertUser:
INSERT INTO SQLUser(id, email, username, isDevAccount)
VALUES(?, ?, ?, ?);

selectUserById:
SELECT *
FROM SQLUser
WHERE id = ?;

selectAllUsersInfo:
SELECT SQLUser.*
FROM SQLUser;

updateUser:
UPDATE SQLUser
SET email = ?,
    username = ?,
    isDevAccount = ?
WHERE id = ?;

removeAllUsers:
DELETE FROM SQLUser;