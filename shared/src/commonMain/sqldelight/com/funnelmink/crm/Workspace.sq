CREATE TABLE SQLWorkspace(
    id TEXT PRIMARY KEY,
    name TEXT NOT NULL,
    role TEXT,
    avatarURL TEXT
);

insertWorkspace:
INSERT INTO SQLWorkspace(id, name, role, avatarURL)
VALUES(?, ?, ?, ?);

selectWorkspaceById:
SELECT *
FROM SQLWorkspace
WHERE id = ?;

selectAllWorkspacesInfo:
SELECT SQLWorkspace.*
FROM SQLWorkspace;

removeAllWorkspaces:
DELETE FROM SQLWorkspace;